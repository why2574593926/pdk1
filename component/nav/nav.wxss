/* component/nav/nav.wxss */
.nav{
  z-index: 999999;
  position: fixed;
  bottom: 0rpx;
  height: 120rpx;
  width: 100%;
  background-color:#FFFFFF;
  box-shadow: 0px 0px 10px rgba(0,0,0,.8);
}
.nav navigator{
  font-size: 40rpx;
  text-align: center;
  float: left;
  width: 25%;
}
navigator image{
  margin-top: 15rpx;
  width: 35rpx;
  height: 35rpx;
}
navigator text{
  display: block;
  color:#AEAEAE;
  font-size: 25rpx;
}
.nav_dingge{
  float: left;
   height: 120rpx;
  width: 100%;
}