/* pages/my/my.wxss */
.friends_max{
  position: fixed;
  overflow: hidden;
  width: 100%;
  height: 100%;
}
.top_js{
  width: 100%;
  height: 80rpx;
  background-color: #FFF7E0;
  color: #FCB177;
  font-size: 30rpx;
  line-height: 80rpx;
}
.top_js image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  margin-top: 20rpx;
  margin-left: 40rpx;
}
.top_js text{
  float: left;
  margin-left: 20rpx;
}
.users{
  width: 90%;
  height: 100rpx;
  margin-left: 5%;
  background-color: white;
  border-bottom: 1rpx solid #F6F6F6;
  line-height: 100rpx;
}
.users image{
  float: left;
  width: 30rpx;
  height: 30rpx;
  margin-top: 35rpx;
}
.users>text{
  margin-left: 20rpx;
  color: #A0A0A0;
}
#text_t{
  color: #5D5D5D;
}
#user1{
  height: 160rpx;
}
#user1 view:nth-of-type(1){
  width: 20%;
  height: 100%;
  border-radius: 50%;
  overflow: hidden;
}
#user1 image{
  width: 100%;
  height: 100%;
}